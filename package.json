    "license": "ISC",
    "devDependencies": {
        "@types/chai": "^5.2.2",
        "@types/mocha": "^10.0.10",
        "@types/node": "^20.9.0",
        "chai": "^5.2.0",
        "glob": "^10.4.5",
        "mocha": "^11.5.0",
        "nodemon": "^3.1.10",
        "nyc": "^15.1.0",
        "rimraf": "^6.0.1",
        "ts-node": "^10.9.1",
        "typescript": "^5.4.5"
    }
}
